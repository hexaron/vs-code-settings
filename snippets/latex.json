{
	"Environment": {
		"prefix": "begin",
		"body": [
			"\\begin{${1:environment}}",
			"$0",
			"\\end{${1:environment}}"
		]
	},
	"Figure with graphics": {
		"prefix": "figure",
		"body": [
			"\\begin{figure}[H]",
			"\t\\centering",
			"\t\\includegraphics[${3:width=0.7\\textwidth}]{$0}",
			"\t\\caption{$1}",
			"\t\\label{fig:$2}",
			"\\end{figure}"
		]
	},
	"Display math environment": {
		"prefix": "display",
		"body": [
			"\\[",
			"\t${0:${TM_SELECTED_TEXT}}",
			"\\]"
		]
	}
}
